{
    "private": true,
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "vite build",
        "preview": "vite preview",
        "dev": "vite",
        "dev:optimized": "vite --mode development --force",
        "dev:css": "concurrently \"pnpm run css\" \"pnpm run dev\"",
        "dev:css:optimized": "concurrently \"pnpm run css\" \"pnpm run dev:optimized\"",
        "dev:css:optimized:watch": "concurrently --kill-others \"pnpm run css:watch\" \"pnpm run dev:optimized\"",
        "sass:watch": "sass --watch --no-source-map resources/scss/custom.scss:resources/css/custom.css resources/scss/theme.scss:resources/css/theme.css",
        "sass:build": "sass --no-source-map resources/scss/custom.scss:resources/css/custom.css resources/scss/theme.scss:resources/css/theme.css --style expanded",
        "scss": "sass --watch --no-source-map resources/scss:resources/css",
        "css": "node scripts/build-css.cjs --clean --minify --inject",
        "css:clean": "node scripts/clean-css.cjs --verbose",
        "css:watch": "node scripts/build-css.cjs --clean --minify --inject --watch",
        "css:watch:verbose": "node scripts/build-css.cjs --clean --minify --inject --watch --verbose",
        "css:verbose": "node scripts/build-css.cjs --clean --minify --inject --verbose",
        "update:docs": "node docs/scripts/generate_index_schema_docs.cjs && node docs/scripts/generate_atomic_index.cjs",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
