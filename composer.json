{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "barryvdh/laravel-dompdf": "^3.1",
        "calcinai/php-imagick": "^0.1.2",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.11",
        "laravel-lang/common": "^6.4",
        "laravel/framework": "^12.00",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "mews/purifier": "^3.4",
        "spatie/laravel-medialibrary": "^11.17",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel-lang/lang": "*",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": "npx concurrently --kill-others \"php artisan serve\" \"php artisan queue:listen\" \"pnpm run dev:css:optimized\"",
        "dev:optimized": "npx concurrently --kill-others \"php artisan serve\" \"php artisan queue:listen\" \"pnpm run dev:css:optimized\"",
        "dev:watch": "npx concurrently --kill-others \"php artisan serve\" \"php artisan queue:listen\" \"pnpm run dev:css:optimized:watch\"",
        "dev:network": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185\" \"php artisan serve --host=0.0.0.0\" \"php artisan queue:listen --tries=1\" \"pnpm run dev\" --names=server,queue,vite"
        ],
        "scss": "sass --watch resources/scss:resources/css",
        "phpstan": "php -d memory_limit=1G ./vendor/bin/phpstan analyse --configuration=phpstan.neon"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
